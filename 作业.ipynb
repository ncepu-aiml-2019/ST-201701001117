    "max_number = 100\n",
    "prime_number_list = []\n",
    "for num in range(3, max_number):\n",
    "    for n in range(2, num//2+1):\n",
    "        if num % n == 0:\n",
    "            break\n",
    "    else:\n",
    "        prime_number_list.append(num)\n",
    "print(prime_number_list)"
